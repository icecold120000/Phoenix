<form id="deleteFact" method="post" action="{{ path('fact_delete', {'id': fact.id,'projectId': project.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce fait ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fact.id) }}">
    <button class="btn btn-danger"><i class="fas fa-trash"></i></button>
</form>
